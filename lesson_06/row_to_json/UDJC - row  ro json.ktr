        <class_name>Processor</class_name>
        <class_source>import org.json.JSONObject;
import java.util.*;
import java.math.BigInteger;


private static final char[] HEX_DIGITS = "0123456789ABCDEF".toCharArray();
public static String toHex(byte[] data) {
    char[] chars = new char[data.length * 2];
    for (int i = 0; i &lt; data.length; i++) {
        chars[i * 2] = HEX_DIGITS[(data[i] >> 4) &amp; 0xf];
        chars[i * 2 + 1] = HEX_DIGITS[data[i] &amp; 0xf];
    }
    return new String(chars);
}
static RowMetaInterface inputRowMeta;


public  Object getKettleValue( Object fieldValue, ValueMetaInterface m_tempValueMeta ) throws KettleException {

    if ( m_tempValueMeta.getType() == ValueMetaInterface.TYPE_BINARY  &amp;&amp; fieldValue != null)
	{	
		String hexaString = toHex((byte[])fieldValue);

		return "\\x" + hexaString;
	}
    return fieldValue;
}



public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException, org.json.JSONException 
{
  Object[] r=getRow();
  String  json_str = "";
  JSONObject  json_value = new JSONObject();
  if (r==null)
  {
    setOutputDone();
	return false;
  }

  if (first) {
	 inputRowMeta = getInputRowMeta();
     first=false;
  }
 
  Object[] outputRowData = RowDataUtil.resizeArray(r, data.outputRowMeta.size());
  String []  fields_name = data.outputRowMeta.getFieldNames();
  ValueMetaInterface valueMetaInterface;
  for (int i = 0; i &lt; getInputRowMeta().size(); i++)
  {	
	valueMetaInterface = inputRowMeta.getValueMeta(i);
	//String test = 	valueMetaInterface.toStringMeta();
    json_value.put(fields_name[i], getKettleValue(r[i], valueMetaInterface));
		//json_value.put(fields_name[i], test); 

  }
  int outputIndex = getInputRowMeta().size();
  outputRowData[outputIndex++] = json_value.toString() ;

  putRow(data.outputRowMeta, outputRowData);

  return true;
}</class_source>
